<div id="widget_{{id}}" class="widget widget_{{widget_type}} widget_size_{{widget_size}}" data-id="{{id}}">
  {{#data}}
    <h2>{{label}}</h2>
    <table cellpadding='0' cellspacing='0'>
      <thead>
        <tr>
          {{#headers}}
            <th>{{.}}</th>
          {{/headers}}
        </tr>
      </thead>
      <tbody>
        {{#rows}}
          <tr>
            {{#row}}
              <td>{{{.}}}</td>
            {{/row}}
          </tr>
        {{/rows}}
        {{^rows}}
          <tr><td></td><td>No Unread Email</td><td></td></tr>
        {{/rows}}
      </tbody>
    </table>
  {{/data}}
</div>